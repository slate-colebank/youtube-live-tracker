{
  "manifest_version": 3,
  "name": "YouTube Live Tracker",
  "version": "1.0.0",
  "description": "A Google Chrome extension used to track your YouTube livestream watchtime.",
  "permissions": ["storage"],
  "action": {
	  "default_icon": {
      "16": "images/icon.png",
      "32": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    },
	  "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*", "*://youtube.com/*"],
      "js": ["dist/content.js"],
			"runAt": "document_end"
    }
  ]
}
